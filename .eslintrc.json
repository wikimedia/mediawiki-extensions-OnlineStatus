{
	"root": true,
	"extends": [
		"wikimedia/client",
		"wikimedia/jquery"
	],
	"globals": {
		"mw": false,
		"OO": false
	},
	"rules": {
		"eqeqeq": "off",
		"no-eval": "off",
		"no-implicit-globals": "off",
		"no-redeclare": "off",
		"no-undef": "off",
		"no-use-before-define": "off",
		"one-var": "off",
		"vars-on-top": "off"
	}
}
